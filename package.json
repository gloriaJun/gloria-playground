{
  "name": "gloria-tilog",
  "private": true,
  "description": "My develop log and story",
  "version": "2.0.0",
  "author": "gloriaJun <pureainu@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/gloriaJun/gloria-tilog"
  },
  "bugs": {
    "url": "https://github.com/gloriaJun/gloria-tilog/issues"
  },
  "keywords": [
    "gatsby",
    "emotion",
    "typescript",
    "storybook",
    "cypress"
  ],
  "scripts": {
    "prepare": "husky install",
    "docusaurus": "docusaurus",
    "start": "docusaurus start",
    "build": "docusaurus build",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids",
    "typecheck": "tsc",
    "tsclint": "tsc --noEmit",
    "prelint": "$npm_execpath run tsclint",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "postlint": "prettier --check \"**/*.{js,jsx,json,md}\"",
    "storybook": "NODE_ENV=production start-storybook -p 6006 -s public",
    "build-storybook": "NODE_ENV=production build-storybook",
    "deploy-storybook": "storybook-to-ghpages",
    "prepreview-storybook": "cross-env $npm_execpath run build-storybook",
    "preview-storybook": "serve storybook-static",
    "test": "jest --coverage --color",
    "test:watch": "jest --watch",
    "test:e2e:run": "start-test serve 8000 cy:run",
    "test:e2e:open": "start-test start 8000 cy:open",
    "precoverage": "$npm_execpath run test:coverage",
    "coverage": "cat ./coverage/lcov.info | coveralls"
  },
  "dependencies": {
    "@docusaurus/core": "2.1.0",
    "@docusaurus/preset-classic": "2.1.0",
    "@mdx-js/react": "^1.6.22",
    "clsx": "^1.2.1",
    "prism-react-renderer": "^1.3.5",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^17.1.0",
    "@docusaurus/eslint-plugin": "^2.1.0",
    "@docusaurus/module-type-aliases": "2.1.0",
    "@tsconfig/docusaurus": "^1.0.5",
    "@typescript-eslint/eslint-plugin": "^5.39.0",
    "@typescript-eslint/parser": "^5.39.0",
    "commitlint": "^17.1.2",
    "coveralls": "^3.1.0",
    "cross-env": "^7.0.2",
    "cypress": "^6.3.0",
    "eslint": "^7.7.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.6",
    "eslint-plugin-react-hooks": "^4.1.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "husky": "^5.1.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.1.2",
    "lint-staged": "^10.2.11",
    "prettier": "^2.1.0",
    "ts-jest": "^29.0.3",
    "typescript": "^4.7.4"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "node": ">=16.14"
  }
}

{
  "name": "gloria-tilog",
  "private": true,
  "description": "My develop log and story",
  "version": "0.1.0",
  "author": "gloriaJun <pureainu@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/gloriaJun/gloria-tilog"
  },
  "bugs": {
    "url": "https://github.com/gloriaJun/gloria-tilog/issues"
  },
  "keywords": [
    "gatsby",
    "emotion",
    "typescript",
    "storybook",
    "cypress"
  ],
  "scripts": {
    "prebuild": "$npm_execpath run test",
    "build": "gatsby build",
    "develop": "gatsby develop",
    "start": "cross-env $npm_execpath run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "lint": "eslint --ignore-path .gitignore . --ext ts --ext tsx --ext js --ext jsx",
    "lint:fix": "cross-env $npm_execpath run lint --fix",
    "prettier:fix": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "prettier:check": "prettier --check .",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "deploy-storybook": "storybook-to-ghpages",
    "prepreview-storybook": "cross-env $npm_execpath run build-storybook",
    "preview-storybook": "serve storybook-static",
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "test:e2e:run": "start-test serve 8000 cy:run",
    "test:e2e:open": "start-test start 8000 cy:open",
    "precoverage": "npm run test:coverage",
    "coverage": "cat ./coverage/lcov.info | coveralls"
  },
  "dependencies": {
    "@emotion/core": "^10.0.27",
    "@emotion/styled": "^10.0.27",
    "@mdx-js/mdx": "^1.6.5",
    "@mdx-js/react": "^1.6.5",
    "@svgr/webpack": "^5.4.0",
    "circular-dependency-plugin": "^5.2.0",
    "clipboard": "^2.0.6",
    "emotion-normalize": "^10.1.0",
    "gatsby": "^2.20.7",
    "gatsby-image": "^2.4.6",
    "gatsby-plugin-alias-imports": "^1.0.5",
    "gatsby-plugin-codegen": "^1.2.0",
    "gatsby-plugin-emotion": "^4.2.1",
    "gatsby-plugin-manifest": "^2.3.3",
    "gatsby-plugin-mdx": "^1.2.13",
    "gatsby-plugin-offline": "^3.1.2",
    "gatsby-plugin-react-helmet": "^3.1.18",
    "gatsby-plugin-sharp": "^2.6.10",
    "gatsby-plugin-svgr": "^2.0.2",
    "gatsby-plugin-typescript": "^2.2.5",
    "gatsby-remark-images": "^3.3.9",
    "gatsby-remark-prismjs": "^3.5.3",
    "gatsby-source-filesystem": "^2.2.2",
    "gatsby-transformer-sharp": "^2.5.4",
    "prismjs": "^1.20.0",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet": "^6.0.0",
    "ts-node": "^8.10.1",
    "typography-theme-moraga": "^0.16.19"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@emotion/babel-preset-css-prop": "^10.0.27",
    "@storybook/addon-actions": "^5.3.17",
    "@storybook/addon-docs": "^5.3.19",
    "@storybook/addon-knobs": "^5.3.17",
    "@storybook/addon-links": "^5.3.17",
    "@storybook/addon-viewport": "^5.3.17",
    "@storybook/addons": "^5.3.14",
    "@storybook/react": "^5.3.17",
    "@storybook/storybook-deployer": "^2.8.3",
    "@testing-library/jest-dom": "^5.9.0",
    "@testing-library/react": "^10.1.0",
    "@types/jest": "^25.2.3",
    "@types/node": "^14.0.5",
    "@typescript-eslint/eslint-plugin": "^3.0.1",
    "@typescript-eslint/parser": "^3.0.1",
    "babel-jest": "^26.0.1",
    "babel-loader": "^8.1.0",
    "babel-plugin-react-docgen": "^4.1.0",
    "babel-preset-gatsby": "^0.4.8",
    "babel-preset-react-app": "^9.1.2",
    "coveralls": "^3.1.0",
    "cross-env": "^7.0.2",
    "cypress": "^4.2.0",
    "eslint": "^7.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^4.0.4",
    "eslint-plugin-simple-import-sort": "^5.0.3",
    "gatsby-plugin-typography": "^2.5.3",
    "husky": "^4.0.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.0.1",
    "lint-staged": "^10.0.9",
    "prettier": "^2.0.5",
    "react-is": "^16.13.1",
    "react-test-renderer": "^16.13.1",
    "react-typography": "^0.16.19",
    "serve": "^11.3.0",
    "start-server-and-test": "^1.10.11",
    "ts-jest": "^26.1.0",
    "typescript": "^3.8.3",
    "typography": "^0.16.19"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "npm run lint:fix",
      "npm run prettier:fix",
      "git add"
    ],
    "{*.{json,md}}": [
      "prettier --write",
      "git add"
    ]
  }
}

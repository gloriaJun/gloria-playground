  "scripts": {
    "prepare": "husky install",
    "start": "cross-env $npm_execpath run develop",
    "develop": "gatsby develop",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "tsclint": "tsc --noEmit",
    "prelint": "$npm_execpath run tsclint",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "postlint": "prettier --check \"**/*.{js,jsx,json,md}\"",

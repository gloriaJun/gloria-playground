  "version": "0.1.0",
  "author": "gloriaJun <pureainu@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/gloriaJun/gloria-tilog"
  },
  "bugs": {
    "url": "https://github.com/gloriaJun/gloria-tilog/issues"
  },
  "keywords": [
    "gatsby",
    "emotion",
    "typescript",
    "storybook",
    "cypress"
  ],
  "scripts": {
    "prebuild": "$npm_execpath run test",
    "build": "gatsby build",
    "develop": "gatsby develop",
    "start": "cross-env $npm_execpath run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "lint": "eslint --ignore-path .gitignore . --ext ts --ext tsx --ext js --ext jsx",
    "lint:fix": "cross-env $npm_execpath run lint --fix",
    "prettier:fix": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "prettier:check": "prettier --check .",
    "storybook": "NODE_ENV=production start-storybook -p 6006 -s public",
    "build-storybook": "NODE_ENV=production build-storybook -s public",
    "deploy-storybook": "storybook-to-ghpages",
    "prepreview-storybook": "cross-env $npm_execpath run build-storybook",
    "preview-storybook": "serve storybook-static",
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "test:e2e:run": "start-test serve 8000 cy:run",
    "test:e2e:open": "start-test start 8000 cy:open",
    "precoverage": "$npm_execpath run test:coverage",
    "coverage": "cat ./coverage/lcov.info | coveralls"
  },
